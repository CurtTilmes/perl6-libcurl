use v6;

use Test;

use LibCurl::Test;
use LibCurl::Easy;

plan 5;

my $injar will leave { unlink $_ } = 'injar46';
my $outjar will leave { unlink $_ } = 'jar46';

spurt $injar,
"# Netscape HTTP Cookie File
# http://curl.haxx.se/docs/http-cookies.html
# This is generated by libcurl!  Do not edit.

www.fake.come	FALSE	/	FALSE	2022144953	cookiecliente	si
www.loser.com	FALSE	/	FALSE	2139150993	UID	99
$HOSTIP	FALSE	/	FALSE	1739150993	mooo	indeed
#HttpOnly_$HOSTIP	FALSE	/want	FALSE	1739150993	mooo2	indeed2
$HOSTIP	FALSE	/want	FALSE	0	empty	
";

my $server = LibCurl::Test.new;

$server.start;

my $curl = LibCurl::Easy.new(URL => "http://$HOSTIP:$HTTPPORT/want/46",
                             cookiefile => $injar,
                             cookiejar => $outjar).perform;

is $curl.response-code, 200, 'response-code';

is $curl.statusline, 'HTTP/1.1 200 OK', 'statusline';

is $server.input,
"GET /want/46 HTTP/1.1
Host: $HOSTIP:$HTTPPORT
Accept: */*
Cookie: empty=; mooo2=indeed2; mooo=indeed

", 'input';

is $curl.content, "This server reply is for testing cookies\n", 'content';

$curl.cleanup;  # Force write of cookie jar

is $outjar.IO.slurp, 
"# Netscape HTTP Cookie File
# http://curl.haxx.se/docs/http-cookies.html
# This file was generated by libcurl! Edit at your own risk.

www.fake.come	FALSE	/	FALSE	2022144953	cookiecliente	si
www.loser.com	FALSE	/	FALSE	2139150993	UID	99
$HOSTIP	FALSE	/	FALSE	1739150993	mooo	indeed
#HttpOnly_$HOSTIP	FALSE	/want	FALSE	1739150993	mooo2	indeed2
$HOSTIP	FALSE	/want	FALSE	0	empty	
$HOSTIP	FALSE	/	FALSE	2054030187	ckyPersistent	permanent
$HOSTIP	FALSE	/	FALSE	0	ckySession	temporary
$HOSTIP	FALSE	/	FALSE	0	ASPSESSIONIDQGGQQSJJ	GKNBDIFAAOFDPDAIEAKDIBKE
$HOSTIP	FALSE	/	FALSE	0	justaname	
$HOSTIP	FALSE	/want/	FALSE	0	simplyhuge	
", 'cookie jar';

done-testing;
